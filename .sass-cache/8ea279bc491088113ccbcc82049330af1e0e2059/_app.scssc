.share-strip{
	overflow: hidden;

	svg{
		@include col(3);
		display: block;
		float: left;
		padding-left: 6px;
	}
}

/*--------------------------------------End Objects / Start Main------------------------------------*/


//body#app
#app{
	background: $white;
	overflow: hidden;
}

aside.app{
	@include col(3);
	height: 100vh;
	float: left;
	border-right: 1px solid nth($grey, 4);
	overflow: hidden;
	// background: $black; /*experimenting with dark bg*/

	iframe{
		background: $black;//remove during tests. Placeholder bg color so video can be commented out
		@include col(3);
		position: fixed;
		bottom: 72px;
		left: 0;
		z-index:1000;
		height: 27px;
		border: none;
	}

	.video-size-ctrl{
		background: $black;
		opacity: .9;
		@include col(3);
		//margin-left: 16.6666667%;
		position: fixed;
		bottom: 72px;
		left: 0;
		z-index:1001;
		height: 27px;
		text-align: right;

		svg{
			height: 27px;
			width: 27px;
			margin-left: 3px;
			cursor: pointer;
		}

		svg:first-child{
			width: 18px;
		}

		svg:last-child{
			width: 36px;
			margin-left: 0;
		}
	}
}

.aside-header{
	// height: 72px;

	.logo{
		@include col(12);
		@include col-ctr(12);
		margin: 12px 0;
	}
}

.li-group:hover, .playlist-nav li:hover, .library-nav li:hover{
	background: nth($grey, 5);
}

.library-nav{
	line-height: 36px;
	font-size: .8em;

	ul{
		li{
			padding: 0 6%;

			a{
				color: nth($grey, 2);
			}
		}
	}
}

.playlist-nav{
	font-size: .8em;

	h3{
		display: inline;
		line-height: 36px;
		padding: 0 6%;
	}

	ul{
		li{
			line-height: 42px;
			cursor: pointer;
			padding: 0 6%;
			position: relative;

			a{
				color: nth($grey, 2);
				@include col(11);
			}

		}

		// li:hover{
		// 	line-height: 63px;
		// }
	}
}

.playlist-menu{
	@include col(1);
	float: right;

	svg{
		height: 12px;
		vertical-align: middle;
	}
}

.add-icon{
	float: right;
}

.playlist-scroll-container::-webkit-scrollbar {
    display: none;
}

.playlist-scroll-container{
	height: 65.25vh;
	overflow: scroll;

-webkit-box-shadow: inset 0px -31px 24px -7px $white;
	   -moz-box-shadow: inset 0px -31px 24px -7px $white;
			box-shadow: inset 0px -31px 24px -7px $white;
}



.popout{
	width: 41%;
	height: 111px;
	background: #fff;
	position: absolute;
	right: 14.5%;
	top: 0px;
	border-radius: 2px;
	padding: 6%;
	z-index: 100;

	-webkit-box-shadow: 7px 7px 11px -5px rgba(24, 26, 26, 0.05);
	   -moz-box-shadow: 7px 7px 11px -5px rgba(24, 26, 26, 0.05);
			box-shadow: 7px 7px 11px -5px rgba(24, 26, 26, 0.05);

	&:before{
		content:"";
		width: 0;
		height: 0;
		border-top: 3px solid transparent;
		border-bottom: 3px solid transparent;
		border-left: 9px solid #fff;
		position: absolute;
		top: 18px;
		right: -9px;
		z-index: 99;
	}

}

.playlist-popout{

	p{
		font-size: .8em;
		margin: 0;
		overflow: hidden;
	}

	.share-strip{
		margin-bottom: 9px;
	}

	.bottom-inner{
		border-top: 1px solid nth($grey, 4);
		padding-top: 12px;
		cursor: pointer;
	}

	svg{
		cursor: pointer;
		height: 18px;
		width: 18px;
	}

	.trash-icon{
		height: 12px;
		width: 12px;
		float: right;
	}
}













/*=====================================*/
/*Main Content container===============*/
/*=====================================*/

section.app{
	@include col(9);
	float: left;
}

.section-header{
	height: 72px; //120
	border-bottom: 1px solid nth($grey, 4);
}

.search{
	overflow: hidden;
	// @include col(8);
	// @include col-ctr(8);
	@include vertical-align;
	// padding: 0 0 2px;

	input{
		float: left;
		margin: 0;
		// padding: 0 6%;
		height: 72px;//39
	}

	input[type="text"]{
		@include col(11);
		border-top-right-radius: 0;
		border-bottom-right-radius: 0;
		// border-right: none;
		border: none;
		font-size: 3.6em;
		font-weight: 100;
		background: transparent;
		padding-left: 3%;
		outline: none;
	}

	input[type="submit"]{
		@include col(1);
		background: $red;
		border-top-left-radius: 0;
		border-bottom-left-radius: 0;
		// border: 1px solid nth($grey, 3);
		border-left: none;
	}

	input[type="submit"]:hover{
		opacity: .8;
	}

	input[type="text"]::-webkit-input-placeholder { /* WebKit browsers */
	    color: nth($grey, 5);
	}
	input[type="text"]:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
	    color: nth($grey, 5);
	}
	input[type="text"]::-moz-placeholder { /* Mozilla Firefox 19+ */
	    color: nth($grey, 5);
	}
	input[type="text"]:-ms-input-placeholder { /* Internet Explorer 10+ */
	    color: nth($grey, 5);
	}
}

.main-container{
	ul{
		margin-bottom: 0;
		position: relative; /*required for submenu overflow visibility*/

		li{
			line-height: 48px;
			padding: 0 2%;
			overflow: hidden;

			.li-play{
				float: left;
			}

			.li-play, .li-add{
				width: 6%;
				display: inline-block;
			}

			.li-title{
				display: inline-block;
				@include col(4);
				float: left;
			}

			.li-artist, .li-album{
				@include col(2);
				display: inline-block;
				float: left;
			}

			.li-length, .li-genre{
				@include col(1);
				float: right;
				display: inline-block;
			}

			.li-add{
				float: right;
				overflow: visible;
				margin-top: 18px;

				svg{
					height: 12px;
					width: 12px;
				}
			}

			svg{
				height: 12px;
				// width: 18px;
				vertical-align: middle;
			}
		}
	}
}


.scroll-container{
	height: 75.25vh;
	overflow: scroll;
}

.li-header{
	font-weight: 700;
	@include col(12);
	border-bottom: 1px solid nth($grey, 4);
	font-size: .8em;

	.li-play{
		// height: 21px;
		line-height: 18px;
		font-size: .6em;
		padding-top: 19px;
		color: nth($grey, 3);

		svg{
			display: block;
		}
	}

	.li-title{
		// margin-left: 6%;
	}

	.li-length{
		margin-right: 6%;
	}
}


.li-group{
	color: nth($grey, 2);
	font-size: .8em;
	cursor: pointer;
}



li.li-group:nth-child(even){
	background: nth($grey, 6);
}




/*=====================================*/
/*li dropdown==========================*/
/*=====================================*/

li.li-group.dropdown{
	background: #fff;
	padding: 18px 18px 18px 9px;
	overflow: hidden;
	// -webkit-box-shadow: inset 0px 3px 12px -2px rgba(24, 26, 26, 0.16);
	//    -moz-box-shadow: inset 0px 3px 12px -2px rgba(24, 26, 26, 0.16);
	// 		box-shadow: inset 0px 3px 12px -2px rgba(24, 26, 26, 0.16);

		img{
			@include col(10);
			margin: 40% 0 0 9px;
			border-radius: 27px;
		}

	.li-desc{
		line-height: 18px;
		margin-left: 9px;
		display: inline-block;
		@include col(6);
	}
}


ul.dropdown-main-menu{
	@include col(2);
	float: right;

	li{
		font-size: .7em;
		line-height: 27px;
	}

	li.li-share{
		margin: 18px 0 0 18px;

		svg{
			height: 21px;
		}
	}
}


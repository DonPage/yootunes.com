/*title attrib hover effect-- work on this later*/
a[title]:hover:after{
	content: attr(title);
	padding: 4px 8px;
	color: $black;
	position: absolute;
	top: 0;
	right: 0;
	white-space: nowrap;
	z-index: 200px;
	border-radius:2px;
	background: $white;
}


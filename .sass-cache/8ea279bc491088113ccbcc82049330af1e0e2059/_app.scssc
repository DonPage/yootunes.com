/*=====================================*/
/*li dropdown==========================*/
/*=====================================*/
/*li.li-group.dropdown{*/
.dropdown{
	/*background: #fff;*/
	padding: 18px 18px 18px 9px;
	overflow: hidden;
	min-height: 240px;
	display: none;//Added to experiment with jquery show/hide on dropdowns
	@include col(12);
	position: relative;

	.youtube-img{//youtube thumbnail
		width: 100%;
		/*height: 100%;*/
		min-height: 102px;
		max-height: 302px;
	}

	.li-desc{
		line-height:1.5;
		font-family: OpenSans;
		font-size: 1.5em;
		margin-left: 9px;
		display: inline-block;
		@include col(6);
	}

	.li-col1{
		@include col(2);
		max-height: 100%;
	}

}

/*Right side menu in dropdown*/
ul.dropdown-main-menu{
	@include col(2);
	float: right;

	li{
		font-size: .7em;
		line-height: 27px;

		.addedToPlaylistCheck{
			height: 12px !important;//overrides main li specificity. Used in jquery affordance
		}
	}

	li.li-share{
		margin: 9px 0 0 24px;
		font-size: .6em;

		.share-strip{
			img.svg:first-child{
				padding-left: 0;
				width: 21px;
			}
		}

		svg, img{
			height: 21px;
			margin-right: 6px;
		}
	}

	li.li-buy{
		svg, img{
			height: 36px;
		}
		margin:0 0 0 18px;
	}
}



/*=====================================*/
/*dropdown-main-menu-popouts===========*/
/*=====================================*/
ul.dropdown-main-menu{
	li{/*do not write styles here. Write them in the above ul.dropdown-main-menu li*/
		ul{
			width: 130%;
			position: absolute;
			right: 105%;
			height: 200px;
			overflow: auto;
			padding: 6% 0 0 6%;
			border-left: 1px solid nth($grey, 6);

			li{/*submenu level*/
				form{
					margin-bottom: 9px;
					margin-top: 0;
					padding-right: 12%;/*mimicks popout padding*/

					input{
						height: 24px;
						font-size: 1.5em;
						margin: 0;
						margin-bottom: 3px;
					}

					input[type="submit"]{
						line-height: 0px;
						background: $green;
						color: $white;
					}
				}
			}
		}
	}
}

ul.dropdown-main-menu li ul::-webkit-scrollbar {
    display: none;
}


#bg-container{
	position: absolute;
}

.popout{
	font-size: .8em;
}

.add-to-playlist-menu{
	background: none;/*overrides popout styles*/
	box-shadow: none;
	padding-right: 0;/*orig: 6% Removed to allow right arrow to hit edge*/

	svg, img{/*more arrow right*/
		float: right;
	}

	p{
		margin-bottom: 15px;
	}

	p:last-child{
		margin-bottom: 3px;
	}

	li.playlist-menu-sub-list{
		font-size: 1.2em;
		line-height: 21px;
	}

	li.playlist-menu-sub-list:hover{
		background: nth($grey, 6);
	}
}

.add-to-playlist-menu-trigger{
	img{
		height: 9px !important; //overrides the add to playlist link icon size set by main-container li
	}
}

 .addedToPlaylistCheck{
 	display: none;
 }


.playlist-menu-header{
	border-bottom: 1px solid nth($grey, 6);
	// margin-bottom: 3px;
}












/*==================================*/
/*Account Settings Page*/
/*==================================*/

.info-wrapper{
	overflow: hidden;
	padding: 6% 6% 12%;
	position: relative;
	min-height: 600px;


	input[type="text"], input[type="password"], input[type="submit"], select, button{
		height: 36px;
	}

	input[type="submit"], button{
		@include col(12);
		height: 45px;
		background: $blue;
		color: $white;
		line-height: 16px;
		border: none;
	}

	input[type="submit"]{
		margin-top: 18px;
	}

	label{
		display: block;
		font-size: 1.2em;
		color: nth($grey, 3);
		margin: 15px 0 6px 0;
	}

	form{
		position: relative;
	}

}

.info-col1, .info-col2, .info-col3{
	@include col(12);
	height: 100%;
	margin-bottom: 72px;

	p{
		font-family: OpenSansBold;
		font-size: 2.1em;
		color: nth($grey, 5);
		border-bottom: 1px solid nth($grey, 1);
	}

	@include app-break-smmd{

		form, #infoDeviceList{
			@include col(4);
		}
	}
}

.second-form{
	margin-top: 48px;
}

.info-col1{
	overflow: hidden;
	margin-bottom: 0;
	padding-bottom: 72px;

	select{
		background: #fff;
		@include col(12);
		padding: 5px;
		font-size: 16px;
		line-height: 1;
		border: 0;
		border-radius: 2px;
		height: 36px;
		-webkit-appearance: none;
		border: 1px solid nth($grey, 3);
		margin-bottom: 9px;
	}
}

#infoId{
	float: right;
	margin-top: 9px;
}

.info-col2, .info-col3{
	padding-bottom: 72px;

	@include app-break-smmd{
		padding-bottom: 72px;
	}
}

.info-col3{

	label[for="emailUpdatesInfo"], label[for="themeDark"], input[type="checkbox"], label[for="disableTips"]{
		display: inline;
		vertical-align: middle;
	}

	button{

		@include app-break-smmd{
			@include col(4);
		}
	}

}

#logoutLink{
	margin-top: 36px;
}

#deleteAccount{
	background: $red;
}


.info-col2{

	ul{
		li{
			overflow: hidden;
			border-top: 1px solid nth($grey, 3);
			line-height: 36px;

			img{
				height: 12px;
				float: right;
				margin-top: 10px;
			}
		}
	}
}


#updateProfileError, #resetPasswordError{
	position: absolute;
	bottom: -36px;
	color: $red;
	z-index: 10000;
	font-size: 1em;
	margin: 0;
	padding: 0;
	border: 0;
}



#resetPasswordError{
	bottom: -18px;
}






/*==================================*/
/*Modal windows*/
/*==================================*/
#deleteAcctModal, #restoreAcctModal, #nameDeviceModal{
	display: none;
}

.modal-container{
	background: rgba(0,0,0,.5);
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	z-index: 10000;
}

.modal{
	@include col(10);
	@include col_ctr(10);
	position: relative;
	background: $white;
	border-radius: 2px;
	margin-top: 13%;
	padding: 1%;
	text-align: center;

	h2{
		font-size: 2.4em;
		margin-bottom: 9px;
	}

	p{
		font-size: 1.2em;
	}

	@include app-break-smmd{
		@include col(3);
		@include col_ctr(3);
	}
}

.modalCloseIcon{
	position: absolute;
	bottom: 102%;
	left: 102%;
}

#deleteAccountButton, #newAccountButton, #restoreAccountButton{
	@include col(8);
	@include col_ctr(8);
	height: 45px;
	color: $white;
	background: $red;
	border: none;
}

#restoreAccountButton{
	background: $green;
}

#restoreAcctModal{
	.modal{
		text-align: left;
	}
}

#devicePrompt{
	p{
		text-align: left;
		margin-top: 18px;
		margin-bottom: 0;
	}
}





#appLoading{
	h3{
		color: $white;
		margin-top: 20%;
		text-align: center;
	}
}




#closeAds{
	position: relative;
	width: 21px;
	height: 21px;
	top: 0;
	left: 92%;
	z-index: 1000;
}

#adsense{
	position: relative;
	/*left: 0;*/
	@include col(12);
	/*bottom: 120px;*/
	height: 61px;
	background: rgba(0,0,0,.5);
	z-index: 10000;
	opacity: .75;
	overflow-x: hidden;
	display: none;


	@include app-break-smmd{
		position: fixed;
		bottom: 72px;
		left: 25%;
		@include col(9);
	}
}

#adsense1{
	width: 468px;
	height: 60px;
	position: absolute;
	left: 0;
	bottom: 0;
}

#adsense2{
	width: 468px;
	height: 60px;
	position: absolute;
	left: 468px;
	bottom: 0;
	display: none;

	@include app-break-sm{
		display: inline-block;
	}
}

#adsense3{
	width: 468px;
	height: 60px;
	position: absolute;
	left: 468px;
	bottom: 0;
	display: none;

	@include app-break-md{
		display: inline-block;
	}
}


.ytbanner {
	width: 468px;
	height: 60px;
	float: left;
	text-align: left;
	left: 0;
}

/*@media(min-width: 500px) { .ytbanner { width: 468px; height: 60px; } }
@media(min-width: 800px) { .ytbanner { width: 728px; height: 90px; } }*/













/*=====================================*/
/*Footer Container=====================*/
/*=====================================*/
.footer{
	background: $black;
	height: 72px;
	position: fixed;
	bottom: 0;
	left: 0;
	right: 0;
	z-index: 1001;
	padding: 0 2%;
	color: $white;
	// overflow: hidden;

	// -webkit-box-shadow: 0px -60px 27px -7px rgba(235,235,235,.8);//$white
	//    -moz-box-shadow: 0px -60px 27px -7px rgba(235,235,235,.8);
	// 		box-shadow: 0px -60px 27px -7px rgba(235,235,235,.8);
}


.info{
	@include col(10);
	float: left;
	overflow: hidden;
	// border-right: 1px solid nth($grey, 1);

	ul{
		float: left;
		margin: 18px 0 0;
		@include col(18);

		li{
			overflow: hidden;
			font-size: .6em;

			@include app-break-smmd{
				font-size: .4em;
			}
		}

		@include app-break-smmd{
			margin: 9px 0 0;
		}
	}

	svg, img{
		display: none;
		float: left;
		height: 36px;
		width: 27px;
		margin: 18px 0 0 0;
		@include col(1);

		@include app-break-smmd{
			display: block;
		}
	}

	@include app-break-smmd{
		@include col(3);
	}
}

/*#infoThumb{
	display: inline;
	width: 48px;
	height: 36px;
	margin-bottom: 21px;
	margin-right: 6%;
}*/

.transport-ctrl{
	@include col(2);
	float: left;
	overflow: hidden;
	// height: 18px;
	height: 100%;
	// border-right: 1px solid nth($grey, 1);

	svg, img{
		@include col(4);
		float: left;
		cursor: pointer;
		@include vertical-align;
	}

	.play{
		max-height: 12px;
		max-width: 12px;
		margin: 0 6px 0 9px;
	}

	.prev, .next{
		max-height: 9px;
		margin-top: 1px;
		display: none;

		@include app-break-smmd{
			display: inline;
		}
	}

	@include app-break-smmd{
		@include col(1);
	}
}

.seek-ctrl{
	position: relative;
	@include col(3);
	top: 36px;
	float: left;
	@include vertical-align;
	height: 36px;
	overflow: hidden;
	z-index: 1007;
	display: none;

	p{
		font-size: 1.2em;
		display: inline;
		float: right;
		line-height: 0px; //important for lining up seek-line
		margin-right: 9px;

		@include app-break-smmd{
			margin-right: 0;
		}
	}

	#seek-bar{
		@include col(10);
		overflow: hidden;
	}

	@include app-break-smmd{
		display: inline-block;

	}
}



.seek-line, .seek-fill, #seek-bar, .seek-buffered{
	@include col(10);
	// @include col-ctr(10);
	height: 0px;
	/*border-bottom: 1px solid nth($grey, 1);*/
	// @include vertical-align;
	margin-top:18px;

	@include app-break-smmd{
		border-bottom: 1px solid nth($grey, 1);
	}
}



.seek-fill{
	width: 0;
	border-bottom: 1px solid $red;
	position: absolute;
}

.seek-buffered{
	width: 0;
	border-bottom: 1px solid nth($grey, 2);
	position: absolute;
}

#seek-dot{
	width: 12px;
	height: 12px;
	// width: 0px;
	// height: 0px;
	background: $white;
	border-radius: 50%;
	position: absolute;
	top: 13px;
	z-index: 10000;
	opacity: .9;
	cursor: pointer;
	// border: 1px solid nth($grey, 2);

	@include app-break-smmd{
		z-index: 10;
	}
}

#seek-dot:active{
	width: 18px;
	height: 18px;
	top: 10px;
}


.playmode-ctrl{
	@include col(1);
	float: left;
	height: 100%;
	padding-left: 2%;
	display: none;

	img{
		@include vertical-align;
		margin-left: 10%;
		cursor: pointer;
		height: 12px;
	}

	@include app-break-smmd{
		display: inline-block;
	}
}

.volume-ctrl{
	@include col(1);
	float: left;
	height: 100%;
	// padding-right: 10%;
	// border-left: 1px solid nth($grey, 1);
	// border-right: 1px solid nth($grey, 1);
	display: none;

	.vol-icon{
		height: 15px;
		cursor: pointer;
		float: left;
		// margin-left: 25%;
		@include vertical-align;
	}

	input{
		@include col(9);
		@include vertical-align;
		display: inline-block;
		margin: 0;
		float: right;
	}

	input[type='range']{
	    -webkit-appearance: none;
	    border: 0;
	    background-color: nth($grey, 2);
	    height: 1px;
	    padding: 0;
	}

	input[type='range']::-moz-range-track {
	    -webkit-appearance: none;
	    border: 0;
	    background-color: nth($grey, 2);
	    height: 1px;
	    padding: 0;
	}

	input[type='range']::-webkit-slider-thumb{
	    -webkit-appearance: none;
	    -moz-appearance: none;
	    width: 6px;
	    height: 6px;
	    -webkit-border-radius: 10px;
	    -moz-border-radius: 10px;
	    border-radius: 10px;
	    background: $white;
	    background-repeat: no-repeat;
	    background-position: 50%;
	}

	input[type='range']::-moz-range-thumb  {
	    -webkit-appearance: none;
	    -moz-appearance: none;
	    width: 6px;
	    height: 6px;
	    -webkit-border-radius: 10px;
	    -moz-border-radius: 10px;
	    border-radius: 10px;
	    background: $white;
	    background-repeat: no-repeat;
	    background-position: 50%;
	}

	input[type='range']::-moz-range-track {
		outline: none;
	/*	color: transparent;
		height: 2px;*/
	}


	input[type='range']::-moz-range-thumb{
		width: 6px;
	    height: 6px;
	    border: none;

	    color: transparent;
	    text-shadow: 0 0 0 $white;
	}


	@include app-break-smmd{
		display: inline-block;
	}

}

.playing-share-ctrl{
	@include col(2);
	float: left;
	height: 100%;
	padding: 0 4%;
	// border-right: 1px solid nth($grey, 1);
	display: none;

	svg, img{
		height: 18px;
		width: 24px;
		margin-top: 25%;
	}

	@include app-break-smmd{
		display: inline-block;
	}
}

.playon-ctrl{
	@include col(12);
	float: right;
	@include vertical-align;

	h3{
		font-size: 1em;
	}

	select#play-on:-moz-focusring, select#mobile-play-on:-moz-focusring {
	    color: transparent;
	    text-shadow: 0 0 0 $white;
	}

	select#play-on, select#mobile-play-on{
		@include col(11);
		background: transparent;
		background: url(../images/icons/down-arrow-wht.svg) no-repeat 90%;
		border-radius: 2px;
		-webkit-appearance: none;
		-moz-appearance: none;
	    text-indent: 0.01px;
	    text-overflow: '';
		border: 1px solid nth($grey, 2);
		color: nth($grey, 3);
		font-size: .9em;
		padding: 3px;
		outline: none;
		height: 27px;
		color: $white;

		option{
			color: $white;
		}
	}

	@include app-break-smmd{
		@include col(1);
	}
}

#mobile-playon{
	margin-top: 102px;

	@include app-break-smmd{
		display: none;
		margin-top: 27px;
	}
}

















/*Light Theme Overrides==============================================*/
.light-fonts{
	color: nth($grey, 3) !important;/*nth($grey, 3)*/
}

.light-border-bottom{
	border-bottom: 1px solid nth($grey, 5) !important;/*nth($grey, 3)*/
}

.light-border-right{
	border-right: 1px solid nth($grey, 5) !important;/*nth($grey, 3)*/
}

.light-border-left{
	border-left: 1px solid nth($grey, 5) !important;/*nth($grey, 3)*/
}

.light-placeholder::-webkit-input-placeholder { /* WebKit browsers */
    color: nth($grey, 5) !important;
}
.light-placeholder:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    color: nth($grey, 5) !important;
}
.light-placeholder::-moz-placeholder { /* Mozilla Firefox 19+ */
    color: nth($grey, 5) !important;
}
.light-placeholder:-ms-input-placeholder { /* Internet Explorer 10+ */
    color: nth($grey, 5) !important;
}

.light-input-bg{
	background: nth($grey, 5) !important;
	border: none !important;
}




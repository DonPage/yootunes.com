.playlist-dropdown{
	background: #fff;
	display: none;//added to help jquery show/hide on load
}

li.playlist-dropdown:hover{
	background: #fff;
}

.playlist-popout{
	padding: 6% 0;

	p{
		font-size: 1em;
		margin: 0;
		overflow: hidden;
	}

	.share-strip{
		margin-bottom: 9px;
	}

	.bottom-inner{
		border-top: 1px solid nth($grey, 4);
		padding-top: 12px;
		cursor: pointer;
	}

	svg, img{
		cursor: pointer;
		height: 36px;
		width: 36px;
	}

	.trash-icon{
		height: 18px;
		width: 18px;
		float: right;
	}
}













/*=====================================*/
/*Main Content container===============*/
/*=====================================*/
section.app{
	@include col(9);
	float: left;
}

.section-header{
	height: 72px; //120
	border-bottom: 1px solid nth($grey, 4);
}

.search{
	overflow: hidden;
	@include vertical-align;

	input{
		float: left;
		margin: 0;
		// padding: 0 6%;
		height: 72px;//39
	}

	input[type="text"]{
		@include col(11);
		border-top-right-radius: 0;
		border-bottom-right-radius: 0;
		// border-right: none;
		border: none;
		font-size: 2.7em;
		font-weight: 100;
		background: transparent;
		padding-left: 3%;
		outline: none;
	}

	input[type="submit"]{
		@include col(1);
		background: transparent;
		border-top-left-radius: 0;
		border-bottom-left-radius: 0;
		border-left: 1px solid nth($grey, 4);
	}

	input[type="submit"]:hover{
		opacity: .8;
	}

	input[type="text"]::-webkit-input-placeholder { /* WebKit browsers */
	    color: nth($grey, 4);
	}
	input[type="text"]:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
	    color: nth($grey, 4);
	}
	input[type="text"]::-moz-placeholder { /* Mozilla Firefox 19+ */
	    color: nth($grey, 4);
	}
	input[type="text"]:-ms-input-placeholder { /* Internet Explorer 10+ */
	    color: nth($grey, 4);
	}
}

.main-container{
	ul{
		margin-bottom: 0;
		position: relative; /*required for submenu overflow visibility*/

		li{
			line-height: 48px;
			padding: 0 2%;
			overflow: hidden;

			svg,img,  .add-icon, .play-icon img{
				height: 12px;
				vertical-align: middle;
			}

			.add-icon{
				width: 12px;
				margin-right: 31%;
			}

			.play-icon{
				img{
					width: 10px;
					margin-left: 23%;
				}
			}
		}
	}
}

.scroll-container{
	height: 75vh;
	overflow: scroll;
}

/*Column widths & floats*/

.li-col1, .li-col2, .li-col3, .li-col4, .li-col5, .li-col6, .li-col7{
	max-height: 48px; //matches li line height
	@extend .truncate;
}

.li-col1{
	float: left;
}

.li-col1, .li-col7{
	width: 6%;
	display: inline-block;
}

.li-col2{
	display: inline-block;
	@include col(4);
	float: left;
	padding-right: 2%;
	margin-right: 2%;
}

.li-col3, .li-col4{
	@include col(2);
	display: inline-block;
	float: left;
}

.li-col3{
	width: 200px;//Testing float fix where content is empty
}

.li-col3:before{
	content: " ";
	width: 200px;
}

.li-col6, .li-col5{
	@include col(1);
	float: right;
	display: inline-block;
}

.li-col6{
	min-width: 8.33333333%;
}

.li-col7{
	float: right;
	margin-top: 18px;

	svg, img{
		height: 12px;
		width: 12px;
		margin-right: 30%;
	}
}

.li-header{
	font-weight: 700;
	@include col(12);
	border-bottom: 1px solid nth($grey, 4);
	font-size: .8em;

	.li-col1{
		// height: 21px;
		line-height: 18px;
		font-size: .6em;
		padding-top: 19px;
		color: nth($grey, 3);

		svg, img{
			display: block;
			margin-left: 24%;
		}
	}

	.li-col7{
		text-align: right;
		margin-top: 0;
	}

	.viewSongs, .viewArtists, .viewAlbums, .viewGenres{
		cursor: pointer;
	}
}

.li-group{
	color: nth($grey, 2);
	font-size: .8em;
	cursor: pointer;
}

li.li-group:nth-child(even){
	background: nth($grey, 6);
}













/*=====================================*/
/*li dropdown==========================*/
/*=====================================*/

li.li-group.dropdown{
	background: #fff;
	padding: 18px 18px 18px 9px;
	overflow: hidden;
	min-height: 150px;
	display: none;//Added to experiment with jquery show/hide on dropdowns

	.youtube-img{//youtube thumbnail
		width: 100%;
		height: 100%;
	}

	.li-desc{
		line-height: 18px;
		margin-left: 9px;
		display: inline-block;
		@include col(6);
	}
}

/*Right side menu in dropdown*/
ul.dropdown-main-menu{
	@include col(2);
	float: right;

	li{
		font-size: .7em;
		line-height: 27px;
	}

	li.li-share{
		margin: 36px 0 0 18px;

		svg, img{
			height: 21px;
		}
	}
}



/*=====================================*/
/*dropdown-main-menu-popouts===========*/
/*=====================================*/
ul.dropdown-main-menu{
	li{/*do not write styles here. Write them in the above ul.dropdown-main-menu li*/
		ul{
			width: 110%;
			position: absolute;
			right: 105%;
			height: 200px;
			overflow: auto;
			padding: 6% 0 0 6%;
			border-left: 1px solid nth($grey, 6);

			li{/*submenu level*/
				form{
					margin-bottom: 9px;
					margin-top: 0;
					padding-right: 12%;/*mimicks popout padding*/

					input{
						height: 24px;
						font-size: .6em;
						margin: 0;
						margin-bottom: 3px;
					}

					input[type="submit"]{
						line-height: 0px;
						background: $green;
						color: $white;
					}
				}
			}
		}
	}
}

ul.dropdown-main-menu li ul::-webkit-scrollbar {
    display: none;
}

.popout{
	font-size: .8em;
}

.add-to-playlist-menu{
	background: none;/*overrides popout styles*/
	box-shadow: none;
	padding-right: 0;/*orig: 6% Removed to allow right arrow to hit edge*/

	svg, img{/*more arrow right*/
		float: right;
	}

	p{
		margin-bottom: 15px;
	}

	p:last-child{
		margin-bottom: 3px;
	}

	li.playlist-menu-sub-list{
		font-size: 1.2em;
		line-height: 21px;
	}

	li.playlist-menu-sub-list:hover{
		background: nth($grey, 6);
	}
}

 .addedToPlaylistCheck{
 	display: none;
 }


.playlist-menu-header{
	border-bottom: 1px solid nth($grey, 6);
	// margin-bottom: 3px;
}

.improve-meta-sub-menu{
	box-shadow: none;
}

.improve-meta-menu-form{

	p{
		margin-top: 27px;
		line-height: 9px;
		margin-bottom: 18px;
	}

	svg, img{
		float: right;
	}
}











/*==================================*/
/*Account Settings Page*/
/*==================================*/
#test-frame{
	width: 1000px;
	height: 590px;
	border: 1px solid black;
}

.info-wrapper{
	overflow: hidden;
	padding: 6%;
	position: relative;

	input[type="text"], input[type="password"], input[type="submit"], select, button{
		height: 36px;
	}

	input[type="submit"], button{
		@include col(12);
		height: 45px;
		background: $blue;
		color: $white;
		line-height: 16px;
		border: none;
	}

	label{
		display: block;
		font-size: 1.2em;
		color: nth($grey, 2);
	}

	.second-form{
		margin-top: 36px;
	}
}

.info-col1, .info-col2, .info-col3{
	@include col(4);
	height: 100%;
	float: left;

	p{
		border-bottom: 1px solid nth($grey, 5);
	}
}

.info-col1{
	overflow: hidden;

	select{
		background: #fff;
		@include col(12);
		padding: 5px;
		font-size: 16px;
		line-height: 1;
		border: 0;
		border-radius: 2px;
		height: 36px;
		-webkit-appearance: none;
		border: 1px solid nth($grey, 3);
		margin-bottom: 9px;
	}
}

#infoId{
	float: right;
	margin-top: 9px;
}

.info-col3{
	padding-left: 6%;


	label[for="emailUpdatesInfo"], input[type="checkbox"]{
		display: inline;
		vertical-align: middle;
	}
}

#logoutLink{
	margin-top: 36px;
}

#deleteAccount{
	background: $red;
}


.info-col2{
	padding-left: 6%;

	ul{
		li{
			overflow: hidden;
			border-top: 1px solid nth($grey, 3);
			line-height: 36px;

			img{
				height: 12px;
				float: right;
				margin-top: 10px;
			}
		}
	}
}










/*==================================*/
/*Modal windows*/
/*==================================*/
#deleteAcctModal, #restoreAcctModal, #nameDeviceModal{
	display: none;
}

.modal-container, #appLoading{
	background: rgba(0,0,0,.9);
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	z-index: 10000;
}

.modal{
	@include col(3);
	@include col_ctr(3);
	position: relative;
	background: $white;
	border-radius: 2px;
	margin-top: 18%;
	padding: 1%;
	text-align: center;

	h2{
		font-size: 2.4em;
		margin-bottom: 9px;
	}

	p{
		font-size: 1.2em;
	}
}

.modalCloseIcon{
	position: absolute;
	bottom: 102%;
	left: 102%;
}

#deleteAccountButton, #newAccountButton, #restoreAccountButton{
	@include col(8);
	@include col_ctr(8);
	height: 45px;
	color: $white;
	background: $red;
	border: none;
}

#restoreAccountButton{
	background: $green;
}

#restoreAcctModal{
	.modal{
		text-align: left;
	}
}





#appLoading{
	h3{
		color: $white;
		margin-top: 20%;
		text-align: center;
	}
}



